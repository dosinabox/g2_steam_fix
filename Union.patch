    #patch [Reset spacer window]
        INT impRegOpenKeyExA   = ImportSymbol("Advapi32.dll", "RegOpenKeyExA")
        INT impRegDeleteValueA = ImportSymbol("Advapi32.dll", "RegDeleteValueA")
        INT impRegCloseKey     = ImportSymbol("Advapi32.dll", "RegCloseKey")
        INT key                = 0
        HEX registryPath       = "SOFTWARE\\PIRANHA-BYTES Software GmbH\\Spacer\\Settings\\\0"
        HEX registryValue      = "winMain\0"
        #assembler [ResetSpacerWindow]
            lea     eax, dword ptr [$key]
            push    eax                     ; key
            push    2                       ; access
            push    0                       ; flags
            push    $registryPath           ; path
            push    0x80000001              ; key
            call    $impRegOpenKeyExA
            cmp     eax, 0
            jnz     return
            push    $registryValue
            push    dword ptr [$key]
            call    $impRegDeleteValueA
            push    dword ptr [$key]
            call    $impRegCloseKey
        return:
            ret
        #/assembler
        ResetSpacerWindow()
    #/patch
#/engine

